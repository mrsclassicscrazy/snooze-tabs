{
  "name": "snooze-tabs",
  "version": "0.1.0",
  "description": "Snooze tabs to open at a specific times in the future",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --watch",
    "chromium": "chromium-browser --load-extension=./dist",
    "opera": "opera --load-extension=./dist",
    "firefox": "web-ext run -s ./dist",
    "production": "rm -rf ./dist & webpack -p && web-ext build -s ./dist"
  },
  "author": "Ramkumar K R",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "less-loader": "^4.0.5",
    "photon-colors": "^1.3.1",
    "proptypes": "^1.1.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.5.4"
  },
  "dependencies": {
    "antd": "^3.0.3",
    "photon-ant": "^0.1.4"
  }
}
